<%= simple_form_for([:admin, course]) do |f| %>
  <%= f.input :name, label: "課程名稱" %>
  <%= f.association :evaluation, collection: Evaluation.all, label_method: :full_name, label: "所屬會期", include_blank: false %>
  <%= f.association :students, collection: students.to_a, as: :check_boxes %>
  <%= f.button :submit %>
<% end %>