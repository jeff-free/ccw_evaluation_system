<%= f.input :name, label: "姓名" %>
<%= f.input :en_name, label: "英文名" %>
<%= f.input :sex, collection: [["男", "male"], ["女", "female"], ["第三性", "third_sex"]], as: :radio_buttons, include_blank: false%>
<%= f.input :tel, label: "聯絡電話" %>
<%= f.input :fax, label: "傳真號碼" %>
<%= f.input :degree, label: "學歷" %>
<%= f.input :avatar_url, label: "圖像" %>
<%= f.input :experience, label: "經歷" %>
<%= f.button :submit, "送出" %>
