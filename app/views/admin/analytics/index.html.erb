
<div class="page-header">
  <h1>委員名單</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>立委姓名</th>
      <th>問卷完成數</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @congressmen.each do |congressman| %>
    <tr>
      <td><%= congressman.name %></td>
      <td><%= congressman.surveys.count %></td>
      <td><%= link_to("查看分析資料", admin_evaluation_analytic_path(@evaluation, congressman)) %></td>
    </tr>
    <% end %>
  </tbody>
</table>